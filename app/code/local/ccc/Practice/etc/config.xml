<?xml version="1.0" ?>
<config>
    <modules>
        <Ccc_Practice>    
            <version>1.0.1</version>
        </Ccc_Practice>
    </modules>
    <frontend>        
        <layout>
            <updates>
                <Practice module="Ccc_Practice">
                    <file>Ccc_Practice.xml</file>
                </Practice>
            </updates>
        </layout>
    </frontend>
    <global>
        <resources>
            <practice_setup>
                <setup>
                    <module>Ccc_Practice</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </practice_setup>
            <practice_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </practice_write>
            <practice_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </practice_read>
        </resources>
        <models>
            <practice>
                <class>Ccc_Practice_Model</class>
                <resourceModel>practice_resource</resourceModel>
            </practice>
            <practice_resource>
                <class>Ccc_Practice_Model_Resource</class>
                <deprecatedNode>practice_mysql4</deprecatedNode>
                <entities>
                    <practice>
                        <table>practice</table>
                    </practice>
                </entities>
            </practice_resource>
        </models>
        <blocks>
            <practice>
                <class>Ccc_Practice_Block</class>
                <!-- <rewrite>
                    <adminhtml_practice>Ccc_Practice_Block_Adminhtml_NewPractice</adminhtml_practice>
                </rewrite> -->
            </practice>

        </blocks>
        <helpers>
           <practice>
                  <class>Ccc_Practice_Helper</class>
            </practice>
            <!-- <practice>
                  <rewrite>
                      <data>Ccc_Practice_Helper_NewData</data>
                  </rewrite>
            </practice> -->
        </helpers>
        <events>
            <cms_page_prepare_save>
                <observers>
                    <Ccc_Practice_Model_Observer>
                        <type>singleton</type>
                        <class>Ccc_Practice_Model_Observer</class>
                        <method>saveCmsPageObserve</method>
                    </Ccc_Practice_Model_Observer>
                </observers>
            </cms_page_prepare_save>
        </events>
    </global>
    <admin>
        <!-- <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Ccc_practice>Ccc_practice_Adminhtml</Ccc_practice>
                    </modules>
                </args>
            </adminhtml>
        </routers> -->
        <routers>
            <practice>
                <use>admin</use>
                <args>
                    <module>Ccc_Practice</module>
                    <frontName>practice</frontName>
                </args>
            </practice>
            <!-- <adminhtml>
                <args>
                    <modules>
                        <ccc_practice before='Mage_Adminhtml'>Ccc_Practice_Adminhtml</ccc_practice>
                    </modules>
                </args>
            </adminhtml> -->
        </routers>
         <layout>
            <updates>
                <practice>
                    <file>Practice.xml</file>
                </practice>
            </updates>
        </layout>
    </admin>
</config>